{
  "name": "nim-rn-mobile-app",
  "version": "1.0.0",
  "description": "React Native app showcasing Nim integration",
  "main": "index.ts",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:nim": "./tools/build-mobile.sh",
    "generate:bindings": "python3 ./tools/generate_bindings.py",
    "build:android": "./tools/build-mobile.sh android",
    "build:ios": "./tools/build-mobile.sh ios",
    "prebuild": "yarn run build:nim"
  },
  "dependencies": {
    "expo": "53",
    "react": "19.0.0",
    "react-native": "0.79.6",
    "typescript": "~5.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli": "^20.0.1",
    "@react-native/babel-preset": "^0.79.5",
    "@react-native/eslint-config": "^0.79.5",
    "@react-native/metro-config": "^0.79.5",
    "@react-native/typescript-config": "^0.79.5",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^19.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "packageManager": "yarn@4.9.4",
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "react-native",
    "nim",
    "mobile",
    "cross-platform"
  ],
  "author": "Nim-RN Team",
  "license": "MIT"
}
